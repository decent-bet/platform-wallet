{
  "name": "dbet-wallet",
  "description": "DBET Wallet",
  "repository": "https://github.com/decent-bet/dbet-wallet",
  "version": "0.2.0",
  "author": {
    "name": "Decent.bet",
    "email": "support@decent.bet"
  },
  "private": true,
  "devDependencies": {
    "@types/react": "^16.4.8",
    "@types/react-dom": "^16.0.7",
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "^22.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-intl": "^2.4.0",
    "babel-preset-react-app": "^3.1.2",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.3.1",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "^0.28.9",
    "detect-port": "1.0.1",
    "electron": "1.8.2",
    "electron-builder": "^20.5.1",
    "electron-packager": "^11.0.1",
    "electron-publisher-s3": "^20.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-react-intl": "^2.0.0",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^5.3.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "2.1.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "filesize": "3.3.0",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "0.17.2",
    "inquirer": "^5.1.0",
    "jest": "^22.4.2",
    "json-loader": "0.5.4",
    "mock-local-storage": "^1.0.5",
    "object-assign": "4.1.0",
    "path-exists": "2.1.0",
    "postcss-loader": "^2.1.0",
    "raf": "^3.4.0",
    "react-dev-utils": "^5.0.0",
    "react-test-renderer": "^16.2.0",
    "recursive-readdir": "2.1.0",
    "strip-ansi": "3.0.1",
    "style-loader": "^0.20.1",
    "truffle-contract": "^1.1.11",
    "truffle-solidity-loader": "0.0.8",
    "url-loader": "^0.5.9",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-manifest-plugin": "^1.3.2",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.3",
    "@fortawesome/fontawesome-free-solid": "^5.0.6",
    "@fortawesome/react-fontawesome": "0.0.17",
    "async": "^2.6.0",
    "bignumber.js": "^7.2.1",
    "bip39": "^2.5.0",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^5.0.1",
    "electron-serve": "^0.1.0",
    "electron-simple-updater": "^1.2.1",
    "electron-updater-appimage-fix": "^1.0.6",
    "ethereum-address": "0.0.4",
    "ethereum-units": "0.0.1-b",
    "ethers": "^2.2.6",
    "ethjs-provider-signer": "^0.1.4",
    "ethjs-signer": "^0.1.1",
    "eventing-bus": "^1.3.3",
    "http2": "^3.3.7",
    "material-ui": "^0.20.0",
    "moment": "^2.20.1",
    "promise": "^8.0.1",
    "react": "^16.4.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.4.2",
    "react-event-components": "^1.1.0",
    "react-fittext": "^1.0.0",
    "react-intl": "^2.4.0",
    "react-material-ui-keyboard": "^6.2.1",
    "react-materialui-notifications": "^0.5.1",
    "react-router-dom": "^4.3.1",
    "react-tap-event-plugin": "^3.0.3",
    "request": "^2.83.0",
    "rxjs": "^6.2.2",
    "scrypt": "^6.0.3",
    "thor-devkit": "^0.2.1",
    "thorify": "0.3.3",
    "web3": "1.0.0-beta.34",
    "web3-eth-abi": "1.0.0-beta.30",
    "web3-eth-accounts": "1.0.0-beta.30",
    "web3-providers-ws": "1.0.0-beta.34",
    "web3-utils": "1.0.0-beta.30"
  },
  "scripts": {
    "start": "node scripts/setup && node scripts/start.js",
    "build": "node scripts/setup && node scripts/build.js",
    "test": "node scripts/setup && node scripts/test.js --env=jsdom",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src",
    "electron": "electron .",
    "pack": "build --dir",
    "dist": "build -wl",
    "postinstall": "./node_modules/.bin/electron-builder install-app-deps"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/src/setupTests.js"
    ],
    "setupTestFrameworkScriptFile": "mock-local-storage",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "ts",
      "tsx",
      "node"
    ]
  },
  "main": "src/electron.js",
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "build": {
    "appId": "wallet.decent.bet",
    "productName": "DBET Wallet",
    "files": [
      "**/*",
      "!dist{/*}",
      "!release-builds",
      "!.idea{/*}",
      "!contracts{/*}",
      "!config{/*}",
      "!migrations{/*}",
      "!node_modules{/*}",
      "!public{/*}",
      "!scripts{/*}",
      "!test{/*}",
      "!truffle-config.js"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "publish": {
        "provider": "s3",
        "bucket": "dbet-wallet",
        "path": "osx",
        "region": "eu-west-2"
      }
    },
    "linux": {
      "icon": "./public/assets/icons/",
      "target": [
        "AppImage"
      ],
      "category": "Network",
      "publish": {
        "provider": "s3",
        "bucket": "dbet-wallet",
        "path": "linux",
        "region": "eu-west-2"
      }
    },
    "win": {
      "icon": "./public/assets/icons/icon-256",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "publish": {
        "provider": "s3",
        "bucket": "dbet-wallet",
        "path": "win",
        "region": "eu-west-2"
      }
    },
    "directories": {
      "buildResources": "public"
    }
  }
}
